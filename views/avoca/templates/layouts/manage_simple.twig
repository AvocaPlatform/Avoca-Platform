<!doctype html>
<html lang="en">
<head>
    {% include 'templates/layouts/header_meta.twig' %}
    <!-- Icons-->
    <link href="{{ avoca_static() }}/vendors/@coreui/icons/css/coreui-icons.min.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/vendors/flag-icon-css/css/flag-icon.min.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/vendors/simple-line-icons/css/simple-line-icons.css" rel="stylesheet">
    <!-- Main styles for this application-->
    <link href="{{ avoca_static() }}/css/style.css" rel="stylesheet">
    <link href="{{ avoca_static() }}/vendors/pace-progress/css/pace.min.css" rel="stylesheet">
    {{ CSS|raw }}

    <script src="{{ avoca_static() }}/vendors/jquery/js/jquery.min.js"></script>
</head>

<body class="app flex-row align-items-center">
    <div class="container">
        {% block content %}{% endblock %}
    </div>

    <!-- necessary plugins-->
    <script src="{{ avoca_static() }}/vendors/popper.js/js/popper.min.js"></script>
    <script src="{{ avoca_static() }}/vendors/bootstrap/js/bootstrap.min.js"></script>
    <script src="{{ avoca_static() }}/vendors/pace-progress/js/pace.min.js"></script>
    <script src="{{ avoca_static() }}/vendors/perfect-scrollbar/js/perfect-scrollbar.min.js"></script>
    <script src="{{ avoca_static() }}/vendors/@coreui/coreui/js/coreui.min.js"></script>
    <!-- Plugins and scripts required by this view-->
    {#<script src="{{ avoca_static() }}/vendors/chart.js/js/Chart.min.js"></script>#}
    <script src="{{ avoca_static() }}/vendors/@coreui/coreui-plugin-chartjs-custom-tooltips/js/custom-tooltips.min.js"></script>
    {{ JS|raw }}
    <script src="{{ avoca_static() }}/js/main.js"></script>
    {% include 'templates/layouts/manage_alert.twig' %}
</body>
</html>